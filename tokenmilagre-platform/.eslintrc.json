{
  "extends": "next/core-web-vitals",
  "ignorePatterns": [
    "lib/generated/**",
    "**/generated/**",
    "**/.prisma/**",
    "node_modules/**",
    ".next/**",
    "*.config.js",
    "*.config.ts"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/no-require-imports": "off",
    "@typescript-eslint/no-this-alias": "warn"
  }
}
